<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/name"
        android:layout_width="200dp"
        android:layout_height="50dp"

        android:layout_marginStart="36dp"
        android:layout_marginBottom="38dp"
        android:fontFamily="sans-serif-black"
        android:hint="car number"
        android:paddingTop="10dp"
        android:textColor="@color/textboxcolor"
        android:textSize="10dp"

        android:theme="@style/txt2"
        app:layout_constraintBottom_toTopOf="@+id/type"
        app:layout_constraintStart_toEndOf="@+id/textView3" />

    <TextView
        android:id="@+id/type"
        android:layout_width="200dp"
        android:layout_height="50dp"


        android:layout_marginStart="32dp"
        android:layout_marginBottom="18dp"
        android:hint="type"
        android:paddingTop="10dp"
        android:textColor="@color/textboxcolor"
        android:textSize="10dp"
        android:theme="@style/txt"
        app:layout_constraintBottom_toTopOf="@+id/quantity"
        app:layout_constraintStart_toEndOf="@+id/textView4" />

    <TextView
        android:id="@+id/quantity"
        android:layout_width="200dp"
        android:layout_height="50dp"

        android:layout_marginStart="31dp"
        android:layout_marginBottom="39dp"
        android:hint="quantity"
        android:paddingTop="10dp"
        android:textColor="@color/textboxcolor"
        android:textSize="10dp"
        android:theme="@style/txt"
        app:layout_constraintBottom_toTopOf="@+id/date"
        app:layout_constraintStart_toEndOf="@+id/textView5" />

    <TextView
        android:id="@+id/date"
        android:layout_width="200dp"
        android:layout_height="50dp"

        android:layout_marginStart="31dp"
        android:layout_marginBottom="33dp"
        android:hint="date"
        android:paddingTop="10dp"
        android:textColor="@color/textboxcolor"
        android:textSize="10dp"
        android:theme="@style/txt"
        app:layout_constraintBottom_toTopOf="@+id/id"
        app:layout_constraintStart_toEndOf="@+id/textView6" />

    <TextView
        android:id="@+id/id"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="51dp"
        android:fontFamily="sans-serif-black"
        android:hint="time"
        android:paddingTop="10dp"
        android:textColor="#F7F3F3"
        android:textSize="10dp"
        android:theme="@style/txt2"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/used"
        app:layout_constraintStart_toEndOf="@+id/textView10" />


    <TextView
        android:id="@+id/used"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="34dp"
        android:layout_marginBottom="209dp"
        android:hint="id"
        android:paddingTop="10dp"
        android:textColor="@color/textboxcolor"
        android:textSize="10dp"
        android:theme="@style/txt"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="141dp"
        android:layout_height="43dp"
        android:layout_marginBottom="36dp"
        android:text="name"
        android:theme="@style/txt"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="141dp"
        android:layout_height="43dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="35dp"
        android:text="type"
        android:theme="@style/txt"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="141dp"
        android:layout_height="43dp"
        android:theme="@style/txt"
        android:layout_marginStart="7dp"
        android:layout_marginBottom="46dp"
        android:text="quantity"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent" />


    <TextView
        android:id="@+id/textView6"
        android:layout_width="141dp"
        android:layout_height="43dp"
        android:theme="@style/txt"
        android:layout_marginStart="7dp"
        android:layout_marginBottom="40dp"
        android:text="date"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="142dp"
        android:layout_height="43dp"
        android:layout_marginBottom="216dp"
        android:text="used"
        android:theme="@style/txt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="142dp"
        android:layout_height="43dp"
        android:layout_marginStart="3dp"
        android:layout_marginBottom="47dp"
        android:text="identification"
        android:theme="@style/txt"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="parent" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="106dp"
        android:theme="@style/txt"/>

    <android.support.constraint.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="148dp"
        android:theme="@style/txt"/>

    <android.support.constraint.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="182dp"
        android:theme="@style/txt"/>

    <android.support.constraint.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="260dp"
        android:theme="@style/txt"/>

    <android.support.constraint.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="349dp"
        android:theme="@style/txt"/>

    <android.support.constraint.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="299dp"
        android:theme="@style/txt"/>

    <android.support.constraint.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="210dp" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="603dp" />

    <TextView
        android:id="@+id/k"
        android:layout_width="142dp"
        android:layout_height="43dp"
        android:layout_marginBottom="148dp"
        android:text="remaining"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/remaining"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="140dp"
        android:text="remaining"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline2" />
    android:theme="@style/txt"
    app:layout_constraintGuide_end="210dp" />


</android.support.constraint.ConstraintLayout>
