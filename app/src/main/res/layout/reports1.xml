<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/name"
        android:layout_width="200dp"
        android:layout_height="50dp"

        android:layout_marginStart="24dp"
        android:layout_marginBottom="28dp"
        android:fontFamily="sans-serif-black"
        android:hint="car number"
        android:paddingTop="10dp"
        android:textColor="@color/textboxcolor"
        android:textSize="10dp"
        android:theme="@style/txt2"
        app:layout_constraintBottom_toTopOf="@+id/trans_type"
        app:layout_constraintStart_toEndOf="@+id/textView3" />

    <TextView
        android:id="@+id/namearriv"
        android:layout_width="200dp"
        android:layout_height="50dp"

        android:layout_marginStart="37dp"
        android:layout_marginTop="27dp"
        android:fontFamily="sans-serif-black"
        android:hint="car number"
        android:paddingTop="10dp"
        android:textColor="@color/textboxcolor"
        android:textSize="10dp"
        android:theme="@style/txt2"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/transid" />

    <TextView
        android:id="@+id/namearriv2"
        android:layout_width="200dp"
        android:layout_height="50dp"

        android:layout_marginStart="37dp"
        android:layout_marginBottom="39dp"
        android:fontFamily="sans-serif-black"
        android:hint="car number"
        android:paddingTop="10dp"
        android:textColor="@color/textboxcolor"
        android:textSize="10dp"
        android:theme="@style/txt2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView11" />

    <TextView
        android:id="@+id/trans_type"
        android:layout_width="200dp"
        android:layout_height="50dp"


        android:layout_marginStart="34dp"
        android:layout_marginBottom="28dp"
        android:hint="type"
        android:paddingTop="10dp"
        android:textColor="@color/textboxcolor"
        android:textSize="10dp"
        android:theme="@style/txt"
        app:layout_constraintBottom_toTopOf="@+id/transquantity"
        app:layout_constraintStart_toEndOf="@+id/textView4" />

    <TextView
        android:id="@+id/transquantity"
        android:layout_width="200dp"
        android:layout_height="50dp"

        android:layout_marginStart="31dp"
        android:layout_marginBottom="39dp"
        android:hint="quantity"
        android:paddingTop="10dp"
        android:textColor="@color/textboxcolor"
        android:textSize="10dp"
        android:theme="@style/txt"
        app:layout_constraintBottom_toTopOf="@+id/transdate"
        app:layout_constraintStart_toEndOf="@+id/textView5" />

    <TextView
        android:id="@+id/transdate"
        android:layout_width="200dp"
        android:layout_height="50dp"

        android:layout_marginStart="31dp"
        android:layout_marginBottom="33dp"
        android:hint="date"
        android:paddingTop="10dp"
        android:textColor="@color/textboxcolor"
        android:textSize="10dp"
        android:theme="@style/txt"
        app:layout_constraintBottom_toTopOf="@+id/transtime"
        app:layout_constraintStart_toEndOf="@+id/textView6" />

    <TextView
        android:id="@+id/transtime"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="34dp"
        android:layout_marginBottom="40dp"
        android:fontFamily="sans-serif-black"
        android:hint="time"
        android:paddingTop="10dp"
        android:textColor="#F7F3F3"
        android:textSize="10dp"
        android:theme="@style/txt2"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/transid"
        app:layout_constraintStart_toEndOf="@+id/textView10" />


    <TextView
        android:id="@+id/transid"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="34dp"
        android:layout_marginBottom="209dp"
        android:hint="id"
        android:paddingTop="10dp"
        android:textColor="@color/textboxcolor"
        android:textSize="10dp"
        android:theme="@style/txt"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="141dp"
        android:layout_height="43dp"
        android:layout_marginBottom="36dp"
        android:text="id"
        android:theme="@style/txt"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="141dp"
        android:layout_height="43dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="35dp"
        android:text="type"
        android:theme="@style/txt"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="141dp"
        android:layout_height="43dp"
        android:layout_marginStart="7dp"
        android:layout_marginBottom="46dp"
        android:text="nature of good"
        android:theme="@style/txt"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent" />


    <TextView
        android:id="@+id/textView6"
        android:layout_width="141dp"
        android:layout_height="43dp"
        android:layout_marginStart="7dp"
        android:layout_marginBottom="40dp"
        android:text="quantity"
        android:theme="@style/txt"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="142dp"
        android:layout_height="43dp"
        android:layout_marginStart="3dp"
        android:layout_marginBottom="209dp"
        android:text="time"
        android:theme="@style/txt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="142dp"
        android:layout_height="43dp"
        android:layout_marginStart="3dp"
        android:layout_marginBottom="47dp"
        android:text="date"
        android:theme="@style/txt"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="142dp"
        android:layout_height="43dp"
        android:layout_marginBottom="60dp"
        android:text="location"
        android:theme="@style/txt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="142dp"
        android:layout_height="43dp"
        android:layout_marginBottom="36dp"
        android:text="typeid"
        android:theme="@style/txt"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintStart_toStartOf="parent" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="106dp"
        android:theme="@style/txt"/>

    <android.support.constraint.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="148dp"
        android:theme="@style/txt"/>

    <android.support.constraint.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="182dp"
        android:theme="@style/txt"/>

    <android.support.constraint.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="260dp"
        android:theme="@style/txt"/>

    <android.support.constraint.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="349dp"
        android:theme="@style/txt"/>

    <android.support.constraint.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="299dp"
        android:theme="@style/txt"/>

    <android.support.constraint.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="210dp" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="609dp" />
    android:theme="@style/txt"
    app:layout_constraintGuide_end="210dp" />


</android.support.constraint.ConstraintLayout>
